{
    "compilerOptions": {
      "jsx": "react-jsx",
      "target": "es6", 
      "module": "es6", 
      "esModuleInterop": true, 
      "forceConsistentCasingInFileNames": true,
      "strict": true ,
      "skipLibCheck": true,
      "outDir": "dist",
      "moduleResolution": "node"
    },
    "include": ["**/*.ts", "**/*.tsx"]
  }